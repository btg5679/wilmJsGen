{
  "name": "wilmjsgen",
  "version": "1.0.0",
  "description": "WilmingtonJS Code-Gen Demo",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run serve",
    "buildAndStart": "tsc && npm run serve",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "generate": "dtsgen --out src/gen/types.d.ts openapi/openapi.yml"
  },
  "author": "Ben Garrison",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^1.18.9",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  },
  "dependencies": {
    "@types/express": "^4.16.1",
    "dtsgenerator": "^2.0.5",
    "express": "^4.16.4"
  }
}
